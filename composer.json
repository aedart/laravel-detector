{
    "name": "aedart/laravel-detector",
    "description": "Utility for detecting if a Laravel application is available or not. Can be used to determine a given component must perform logic based current Laravel application settings or not.",
    "keywords": [
        "Detector",
        "Laravel",
        "Utility"
    ],
    "homepage": "https://bitbucket.org/aedart/laravel-detector",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "aedart/license": "1.*",
        "aedart/license-file-manager": "1.*",
        "aedart/testing-laravel": "1.0.*",
        "codeception/codeception": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Laravel\\Detector\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-update-cmd": "php vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
    }
}