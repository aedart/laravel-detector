{
    "name": "aedart/laravel-detector",
    "description": "Utility for detecting if a Laravel application is available or not. Can be used to determine if a given component must perform logic based current Laravel application settings or not.",
    "keywords": [
        "Detector",
        "Laravel",
        "Utility"
    ],
    "homepage": "https://github.com/aedart/laravel-detector",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": "5.4.*"
    },
    "require-dev": {
        "aedart/license": "1.*",
        "aedart/license-file-manager": "~1.7",
        "aedart/testing-laravel": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Laravel\\Detector\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}